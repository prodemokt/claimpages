function FsmContext() {
	this.renderer = '';
	this.oldfsmobj = '';
	this.oldrenderer = '';
	this.status = false;

	this.setRenderer=function(renderer){
		this.renderer = renderer;
	}

	this.getRenderer=function(){
		return this.renderer;
	}
	
	this.clearRenderer=function(){
	    this.renderer = '';
	}
	
	this.updateObj = function(obj){
		this.oldfsmobj = obj;
	}
	
	this.rollbackObj = function (){		
		fsmCont.init(this.oldfsmobj.MACH_NAME, this.oldfsmobj.STATE_NAME, this.oldfsmobj.MACH_PROC_ID, this.oldfsmobj.MACH_SEQ_NO , this.oldfsmobj.MACH_AUTH_ID);
		
	}
	
	this.updateoldrenderer = function(cxtrenderer){
		this.oldrenderer = cxtrenderer;
	}
	
	this.rollbackoldrenderer = function (){		
		this.renderer = this.oldrenderer;		
	}
	
	this.setModelStatus = function(status){
		this.status = status;
	}
	
	this.isModelOpen=function(){
		return this.status;
	}

}
var fsmcxt = new FsmContext();

 